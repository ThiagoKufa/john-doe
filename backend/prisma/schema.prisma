
generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Customer {
  id            Int      @id @default(autoincrement())
  fullName      String
  cpf           String
  email         String
  favoriteColor String
  observation   String?
}